{
  "name": "gms-frontend",
  "version": "1.0.4-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "startHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration localHttps",
    "mockHttp": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration mockHttp",
    "build": "ng build --output-hashing none",
    "watch": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration development",
    "watchHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration localHttps",
    "test": "jest --config=jest.config.js --silent=false --maxWorkers=25% --maxConcurrency=8",
    "clearTestCache": "jest --clearCache",
    "coverage": "test --coverage --detectOpenHandles",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --ext .ts . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.3",
    "@angular/cdk": "17.3.3",
    "@angular/common": "17.3.3",
    "@angular/compiler": "17.3.3",
    "@angular/core": "17.3.3",
    "@angular/forms": "17.3.3",
    "@angular/material": "17.3.3",
    "@angular/platform-browser": "17.3.3",
    "@angular/platform-browser-dynamic": "17.3.3",
    "@angular/router": "17.3.3",
    "@angular/service-worker": "17.3.3",
    "crypto-js": "^4.2.0",
    "follow-redirects": "1.15.6",
    "jest-environment-jsdom": "^29.7.0",
    "moment": "^2.29.4",
    "randomstring": "1.3.0",
    "rxjs": "7.8.1",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "webpack-dev-middleware": "7.1.1",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.5",
    "@angular/cli": "17.3.3",
    "@angular/compiler-cli": "17.3.3",
    "@types/crypto-js": "^4.1.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.12.5",
    "@types/randomstring": "1.3.0",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "8.41.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-promise": "6.1.1",
    "follow-redirects": "1.15.6",
    "jest": "^29.7.0",
    "jest-html-reporter": "3.10.2",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "^14.0.3",
    "typescript": "^5.2.2",
    "webpack-dev-middleware": "7.1.1"
  },
  "author": {
    "name": "Peter Szrnka",
    "email": "szrnka.peter@gmail.com",
    "url": "https://github.com/peter-szrnka"
  }
}
