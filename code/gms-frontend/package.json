{
  "name": "gms-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "startHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration localHttps",
    "build": "ng build --output-hashing none",
    "watch": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration development",
    "watchHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration localHttps",
    "test": "ng test --silent=false",
    "coverage": "test --coverage --detectOpenHandles",
    "lint": "eslint --ext .ts . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.15",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.15",
    "@angular/compiler": "12.2.15",
    "@angular/core": "12.2.15",
    "@angular/forms": "12.2.15",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.15",
    "@angular/platform-browser-dynamic": "12.2.15",
    "@angular/router": "12.2.15",
    "@angular/service-worker": "12.2.15",
    "crypto-js": "^4.1.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "3.1.2",
    "moment": "^2.29.4",
    "rxjs": "7.5.0",
    "tslib": "^2.3.0",
    "uuid": "8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.0.0",
    "@angular-devkit/build-angular": "15.1.0",
    "@angular/cli": "12.2.16",
    "@angular/compiler-cli": "12.2.15",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "26.1.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "26.0.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "13.13.5",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.8.0",
    "eslint-config-standard-with-typescript": "16.0.0",
    "eslint-plugin-jest": "23.9.0",
    "eslint-plugin-promise": "4.2.1",
    "jest": "26.6.3",
    "jest-html-reporter": "3.7.0",
    "jest-junit": "14.0.1",
    "jest-preset-angular": "8.3.2",
    "ts-jest": "26.5.4",
    "typescript": "4.3.5"
  },
  "author": {
    "name": "Peter Szrnka",
    "email": "szrnka.peter@gmail.com"
  }
}
