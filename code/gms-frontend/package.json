{
  "name": "gms-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "startHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration localHttps",
    "build": "ng build --output-hashing none",
    "watch": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration development",
    "watchHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration localHttps",
    "test": "jest --config=jest.config.js --silent=false",
    "clearTestCache" : "jest --clearCache",
    "coverage": "test --coverage --detectOpenHandles",
    "lint": "eslint --ext .ts . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.12",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.2.12",
    "@angular/compiler": "14.2.12",
    "@angular/core": "14.2.12",
    "@angular/forms": "14.2.12",
    "@angular/material": "14.2.7",
    "@angular/platform-browser": "14.2.12",
    "@angular/platform-browser-dynamic": "14.2.12",
    "@angular/router": "14.2.12",
    "@angular/service-worker": "14.2.12",
    "crypto-js": "^4.1.1",
    "jest-environment-jsdom": "^28.1.3",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "3.1.2",
    "moment": "^2.29.4",
    "rxjs": "7.5.7",
    "ts-jest": "^28.0.8",
    "tslib": "^2.4.1",
    "uuid": "8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular/cli": "14.2.10",
    "@angular/compiler-cli": "14.2.12",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.18.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "^7.8.0",
    "eslint-config-standard-with-typescript": "^16.0.0",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^28.0.0",
    "jest-html-reporter": "3.7.0",
    "jest-junit": "14.0.1",
    "jest-preset-angular": "12.2.4",
    "typescript": "4.8.4"
  },
  "author": {
    "name": "Peter Szrnka",
    "email": "szrnka.peter@gmail.com"
  }
}