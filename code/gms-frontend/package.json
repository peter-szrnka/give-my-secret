{
  "name": "gms-frontend",
  "version": "1.0.4-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration localHttp",
    "startHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration localHttps",
    "mockHttp": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration mockHttp",
    "startUiTestFrontend": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --port 4199 --configuration mockHttp",
    "build": "ng build --output-hashing none",
    "buildLocal": "ng build --output-hashing none --configuration productionLocal",
    "startLocal": "angular-http-server dist/gms-frontend -p 4200 --baseHref gms-app --rootFile dist/gms-frontend/index.html",
    "watch": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration development",
    "watchHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration localHttps",
    "test": "jest --config=jest.config.js --silent=false --maxWorkers=25% --maxConcurrency=8",
    "uiTest": "npx playwright test",
    "showUiReport": "npx playwright show-report",
    "clearTestCache": "jest --clearCache",
    "coverage": "npm run test --coverage --detectOpenHandles",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --ext .ts . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.5",
    "@angular/cdk": "18.2.5",
    "@angular/common": "18.2.5",
    "@angular/compiler": "18.2.5",
    "@angular/core": "18.2.5",
    "@angular/forms": "18.2.5",
    "@angular/material": "18.2.5",
    "@angular/platform-browser": "18.2.5",
    "@angular/platform-browser-dynamic": "18.2.5",
    "@angular/router": "18.2.5",
    "@angular/service-worker": "18.2.5",
    "body-parser": "^1.20.3",
    "cookie": ">=0.7.0",
    "crypto-js": "^4.2.0",
    "express": "^4.20.0",
    "follow-redirects": "1.15.6",
    "jest-environment-jsdom": "^29.7.0",
    "moment": "^2.29.4",
    "randomstring": "1.3.0",
    "rollup": "4.22.4",
    "rxjs": "7.8.1",
    "send": "^0.19.0",
    "serve-static": "^1.16.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "vite": "5.4.19",
    "webpack-dev-middleware": "7.1.1",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.18",
    "@angular/cli": "18.2.5",
    "@angular/compiler-cli": "18.2.5",
    "@playwright/test": "^1.49.0",
    "@types/crypto-js": "^4.1.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.12.5",
    "@types/randomstring": "1.3.0",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "body-parser": "^1.20.3",
    "cookie": ">=0.7.0",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "8.41.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-promise": "6.1.1",
    "express": "^4.20.0",
    "follow-redirects": "^1.15.6",
    "jest": "^29.7.0",
    "jest-html-reporter": "3.10.2",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "^14.0.3",
    "rollup": "4.22.4",
    "send": "^0.19.0",
    "serve-static": "^1.16.0",
    "typescript": "^5.5.4",
    "vite": "5.4.19",
    "webpack-dev-middleware": "7.1.1"
  },
  "author": {
    "name": "Peter Szrnka",
    "email": "szrnka.peter@gmail.com",
    "url": "https://github.com/peter-szrnka"
  }
