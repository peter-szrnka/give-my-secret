{
  "name": "gms-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "startHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration localHttps",
    "build": "ng build --output-hashing none",
    "watch": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration development",
    "watchHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration localHttps",
    "test": "jest --config=jest.config.js --silent=false --maxWorkers=25% --maxConcurrency=8",
    "clearTestCache": "jest --clearCache",
    "coverage": "test --coverage --detectOpenHandles",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --ext .ts . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.9",
    "@angular/cdk": "15.2.9",
    "@angular/common": "15.2.9",
    "@angular/compiler": "15.2.9",
    "@angular/core": "15.2.9",
    "@angular/forms": "15.2.9",
    "@angular/material": "15.2.9",
    "@angular/platform-browser": "15.2.9",
    "@angular/platform-browser-dynamic": "15.2.9",
    "@angular/router": "15.2.9",
    "@angular/service-worker": "15.2.9",
    "crypto-js": "^4.2.0",
    "jest-environment-jsdom": "^28.1.3",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "3.1.2",
    "moment": "^2.29.4",
    "randomstring": "^1.3.0",
    "rxjs": "7.5.7",
    "ts-jest": "^28.0.8",
    "tslib": "^2.6.2",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.10",
    "@angular/cli": "15.2.10",
    "@angular/compiler-cli": "15.2.9",
    "@types/crypto-js": "^4.1.3",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.18.59",
    "@types/randomstring": "^1.1.10",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "8.41.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "^28.1.3",
    "jest-html-reporter": "3.7.0",
    "jest-junit": "14.0.1",
    "jest-preset-angular": "12.2.4",
    "typescript": "4.9.3"
  },
  "author": {
    "name": "Peter Szrnka",
    "email": "szrnka.peter@gmail.com",
    "url": "https://github.com/peter-szrnka"
  }
}
