{
  "name": "gms-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "startHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration localHttps",
    "build": "ng build --output-hashing none",
    "watch": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration development",
    "watchHttps": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --watch --configuration localHttps",
    "test": "jest --config=jest.config.js --silent=false",
    "coverage": "test --coverage --detectOpenHandles",
    "lint": "eslint --ext .ts . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.12",
    "@angular/cdk": "13.3.9",
    "@angular/common": "13.3.12",
    "@angular/compiler": "13.3.12",
    "@angular/core": "13.3.12",
    "@angular/forms": "13.3.12",
    "@angular/material": "13.3.9",
    "@angular/platform-browser": "13.3.12",
    "@angular/platform-browser-dynamic": "13.3.12",
    "@angular/router": "13.3.12",
    "@angular/service-worker": "13.3.12",
    "crypto-js": "^4.1.1",
    "jest-config": "^28.1.3",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "3.1.2",
    "moment": "^2.29.4",
    "rxjs": "7.5.0",
    "tslib": "^2.3.0",
    "uuid": "8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.3",
    "@angular-devkit/build-angular": "13.3.10",
    "@angular/cli": "13.3.10",
    "@angular/compiler-cli": "13.3.12",
    "@jest/globals": "28.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "28.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "13.13.5",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.8.0",
    "eslint-config-standard-with-typescript": "16.0.0",
    "eslint-plugin-jest": "23.9.0",
    "eslint-plugin-promise": "4.2.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.3.1",
    "jest-html-reporter": "3.7.0",
    "jest-junit": "14.0.1",
    "jest-preset-angular": "12.1.0-next.0",
    "ts-jest": "28.0.8",
    "typescript": "4.6.4"
  },
  "author": {
    "name": "Peter Szrnka",
    "email": "szrnka.peter@gmail.com"
  }
}
